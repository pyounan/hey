  "swagger": "2.0",
  "info": {
    "description": "pos-proxy acts as the backend for POS module in the localnetwork\nit handles all the POS operations also handles the offline mode\nscenarios",
    "title": "pos-proxy api",
    "version": "1.0.0"
  },
  "host": "localhost",
  "basePath": "/api/",
  "paths": {
    "/api/ensure_tenant_selected/": {
      "get": {
        "description": "in case of proxy always returns true",
        "tags": [
          "auth"
        ],
        "summary": "Ensure Tenant Selected",
        "operationId": "ensureTenantSelected"
      }
    },
    "/api/pos/posinvoices": {
      "get": {
        "description": "serves a list of simplified invoices with basic\ninformation",
        "tags": [
          "invoices"
        ],
        "summary": "List Invoices (simplified)",
        "operationId": "listInvoicesLite",
        "parameters": [
          {
            "name": "simplified",
            "in": "query",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "invoiceLite",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/invoiceLite"
              }
            }
          }
        }
      }
    },
    "/core/getloggedinusergroups/{id}/": {
      "get": {
        "description": "returns a list of permissions assigned for the logged in user",
        "tags": [
          "auth"
        ],
        "summary": "List User Permissions",
        "operationId": "getUserPermissions",
        "parameters": [
          {
            "type": "integer",
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ]
      }
    },
    "/income/api/cashier/clockout/": {
      "post": {
        "description": "logs out a cashier and create a clockout record in the attendance sheet\nreturns a 200 success with true as body\n\nParamters:\n+ name: request body\nin: body\nrequired: true\ntype: clockoutRequest",
        "tags": [
          "income"
        ],
        "summary": "Clockout POS Cashier",
        "operationId": "clockout"
      }
    },
    "/income/api/cashier/getposcashier/": {
      "post": {
        "description": "used to clockin a cashier and compare his\npin against the correct one from the database",
        "tags": [
          "income"
        ],
        "summary": "Log in a POS cashier",
        "operationId": "getPOSCashier",
        "parameters": [
          {
            "type": "object",
            "name": "request body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "$ref": "#/definitions/clockinRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "cashier",
            "schema": {
              "$ref": "#/definitions/cashier"
            }
          }
        }
      }
    },
    "/income/api/currency/": {
      "get": {
        "description": "returns a list with available currencies",
        "tags": [
          "income"
        ],
        "summary": "List Currencies",
        "operationId": "getCurrencyList",
        "responses": {
          "200": {
            "description": "currency",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/currency"
              }
            }
          }
        }
      }
    },
    "/income/api/currency/{id}/": {
      "get": {
        "description": "returns a details of a currency by id\n\nParamters:\n+ name: id\nin: path\nrequired: true\nschema:\ntype: integer",
        "tags": [
          "income"
        ],
        "summary": "Get Currency",
        "operationId": "getCurrency",
        "responses": {
          "200": {
            "description": "currency",
            "schema": {
              "$ref": "#/definitions/currency"
            }
          }
        }
      }
    },
    "/income/api/department/": {
      "get": {
        "description": "returns a list of income departments",
        "tags": [
          "income"
        ],
        "summary": "List Departments",
        "operationId": "getDepartmentList",
        "responses": {
          "200": {
            "description": "department",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/department"
              }
            }
          }
        }
      }
    },
    "/income/api/department/{id}/": {
      "get": {
        "description": "returns a details of a department by id\n\nParamters:\n+ name: id\nin: path\nrequired: true\nschema:\ntype: integer",
        "tags": [
          "income"
        ],
        "summary": "Get Department",
        "operationId": "getDepartment",
        "responses": {
          "200": {
            "description": "department",
            "schema": {
              "$ref": "#/definitions/department"
            }
          }
        }
      }
    },
    "/income/api/poscashierpermissions/": {
      "get": {
        "description": "return a json http response contains list of\npermissions assigned to a certain cashier",
        "tags": [
          "income"
        ],
        "summary": "List Cashier Permissions",
        "operationId": "permissions",
        "responses": {
          "200": {
            "description": "cashierPermissions",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/cashierPermissions"
              }
            }
          }
        }
      }
    },
    "/proxy/test": {
      "get": {
        "description": "returns a success message if the proxy is working properly.",
        "tags": [
          "proxy"
        ],
        "summary": "Test Proxy (Check Status)",
        "operationId": "proxyStatus"
      }
    },
    "/proxy/version": {
      "get": {
        "description": "returns the version of the proxy",
        "tags": [
          "proxy"
        ],
        "summary": "Get Proxy Version",
        "operationId": "proxyVersion",
        "responses": {
          "200": {
            "description": "proxyVersion",
            "schema": {
              "$ref": "#/definitions/proxyVersion"
            }
          }
        }
      }
    },
    "/shadowinn/api/auditdate/": {
      "get": {
        "description": "returns the current audit date of the instance",
        "tags": [
          "shadowinn"
        ],
        "summary": "Get Audit Date",
        "operationId": "auditDate",
        "responses": {
          "200": {
            "description": "auditDateResponse",
            "schema": {
              "$ref": "#/definitions/auditDateResponse"
            }
          }
        }
      }
    }
  },
